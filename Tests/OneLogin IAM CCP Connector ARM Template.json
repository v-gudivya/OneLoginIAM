                      },
                      {
                        "name": "created_at",
                        "type": "datetime"
                      }
                    ]
                  },
                  "Custom-OneLoginUsersV2_CL": {
                    "columns": [
                      {
                        "name": "username",
                        "type": "string"
                      },
                      {
                        "name": "firstname",
                        "type": "string"
                      },
                      {
                        "name": "email",
                        "type": "string"
                      },
                      {
                        "name": "lastname",
